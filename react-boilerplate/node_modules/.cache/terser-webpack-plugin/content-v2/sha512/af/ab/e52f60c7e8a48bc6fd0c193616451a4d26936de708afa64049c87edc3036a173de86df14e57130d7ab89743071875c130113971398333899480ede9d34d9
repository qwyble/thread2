, props) {\\n    const Component = loaded.default;\\n    return <Component {...props} />;\\n  },\\n});\\n\\nconst LoadableProtected = Loadable({\\n  loader: () => import('../../containers/authentication/protected.js'),\\n  loading: Loader,\\n  render(loaded, props) {\\n    const Component = loaded.default;\\n    return <Component {...props} />;\\n  },\\n});\\n\\nclass UserContainer extends React.Component {\\n  state = {\\n    user: {},\\n    isLoggedIn: false,\\n    isLoading: true,\\n  };\\n\\n  // check with server to see if user is logged in\\n  componentDidMount() {\\n    this.setState({ isLoading: true });\\n    axios({\\n      method: 'get',\\n      url: 'https://thread-204819.appspot.com/auth',\\n      withCredentials: true,\\n    }).then(result => {\\n      if (result.data.idUsers) {\\n        this.setState({\\n          user: result.data,\\n          isLoggedIn: true,\\n          isLoading: false,\\n        });\\n      } else {\\n        this.setState({ isLoading: false });\\n      }\\n    });\\n  }\\n\\n  handleLogin = (user, isLoggedIn) => {\\n    this.setState({ user, isLoggedIn });\\n  };\\n\\n  handleLogout = () => {\\n    axios({\\n      method: 'post',\\n      url: 'https://thread-204819.appspot.com/logout',\\n      withCredentials: true,\\n    }).then(result => {\\n      this.setState({ user: {}, isLoggedIn: false });\\n    });\\n  };\\n\\n  render() {\\n    if (this.state.isLoading) {\\n      return (\\n        <div>\\n          <Loader style={{ marginTop: '30vh' }} active inline=\\\"centered\\\" />\\n        </div>\\n      );\\n    }\\n    return (\\n      <BrowserRouter>\\n        <div>\\n          <Switch>\\n            <Route\\n              path=\\\"/stream\\\"\\n              render={props => (\\n                <LoadableProtected\\n                  {...props}\\n                  isLoggedIn={this.state.isLoggedIn}\\n                  onLogout={this.handleLogout}\\n                  user={this.state.user}\\n                />\\n             